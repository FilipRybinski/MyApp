{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "url": {
    "allowedHeaders": [
      "application/json",
      "text/plain",
      "Content-Type",
      "Referer",
      "Sec-Ch-Ua",
      "Sec-Ch-Ua-Mobile",
      "Sec-Ch-Ua-Platform",
      "User-Agent"
    ],
    "allowedMethods": [
      "POST",
      "PUT",
      "DELETE",
      "PATCH",
      "GET"
    ],
    "connectionUrls": ["http://localhost:4200","http://localhost:4000"]
  },
  "featureFlags": {
    "Dashboard" : "true",
    "Finance" : "false",
    "Management" : "false",
    "Marketplace": "false"
  }
}
